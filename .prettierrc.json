{
  "printWidth": 100,
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "attributeGroups": [
    "^#",
    "^:?is$",
    "^v-for$",
    "^v-(if|else-if|else)$",
    "^v-show$",
    "^v-cloak$",
    "^v-pre$",
    "^v-once$",
    "^:?id$",
    "^:?ref$",
    "^:?key$",
    "^v-model$",
    "^v-model:",
    "^:?class$",
    "^:(?!data-)",
    "$DEFAULT",
    "^:?data-",
    "^@",
    "^v-html$",
    "^v-text$",
    "^v-bind$"
  ],
  "plugins": [
    "prettier-plugin-tailwindcss"
  ]
}